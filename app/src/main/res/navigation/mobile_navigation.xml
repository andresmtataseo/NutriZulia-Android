<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/inicioFragment">
    <fragment
        android:id="@+id/consultasFragment"
        android:name="com.nutrizulia.presentation.view.ConsultasFragment"
        android:label="Consultas"
        tools:layout="@layout/fragment_consultas">
        <action
            android:id="@+id/action_consultasFragment_to_seleccionarPacienteCitaFragment"
            app:destination="@id/seleccionarPacienteCitaFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_consultasFragment_to_accionesConsultaFragment"
            app:destination="@id/accionesConsultaFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_consultasFragment_to_accionesCitaFragment"
            app:destination="@id/accionesCitaFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_consultasFragment_to_accionesCitaPerdidaFragment"
            app:destination="@id/accionesCitaPerdidaFragment" />
    </fragment>
    <fragment
        android:id="@+id/inicioFragment"
        android:name="com.nutrizulia.presentation.view.InicioFragment"
        android:label="Inicio"
        tools:layout="@layout/fragment_inicio">
        <action
            android:id="@+id/action_inicioFragment_to_pacientesFragment"
            app:destination="@id/pacientesFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_inicioFragment_to_reportesFragment"
            app:destination="@id/reportesFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_inicioFragment_to_consultasFragment"
            app:destination="@id/consultasFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_inicioFragment_to_ayudaFragment"
            app:destination="@id/ayudaFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_inicioFragment_to_cuentaFragment"
            app:destination="@id/cuentaFragment" />
        <action
            android:id="@+id/action_inicioFragment_to_seleccionarInstitucionFragment2"
            app:destination="@id/seleccionarInstitucionFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/pacientesFragment"
        android:name="com.nutrizulia.presentation.view.PacientesFragment"
        android:label="Pacientes"
        tools:layout="@layout/fragment_pacientes">
        <action
            android:id="@+id/action_pacientesFragment_to_registrarPacienteFragment"
            app:destination="@id/registrarPacienteFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpToInclusive="false" />
        <action
            android:id="@+id/action_pacientesFragment_to_accionesPacienteFragment"
            app:destination="@id/accionesPacienteFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/reportesFragment"
        android:name="com.nutrizulia.presentation.view.ReportesFragment"
        android:label="Reportes"
        tools:layout="@layout/fragment_reportes" />
    <fragment
        android:id="@+id/cuentaFragment"
        android:name="com.nutrizulia.presentation.view.CuentaFragment"
        android:label="Cuenta"
        tools:layout="@layout/fragment_cuenta" />
    <fragment
        android:id="@+id/ayudaFragment"
        android:name="com.nutrizulia.presentation.view.AyudaFragment"
        android:label="Ayuda"
        tools:layout="@layout/fragment_ayuda" />
    <fragment
        android:id="@+id/registrarPacienteFragment"
        android:name="com.nutrizulia.presentation.view.RegistrarPacienteFragment"
        android:label="Datos del Paciente"
        tools:layout="@layout/fragment_registrar_paciente">
        <argument
            android:name="pacienteId"
            app:argType="string"
            app:nullable="true"
            android:defaultValue="null" />
        <argument
            android:name="isEditable"
            app:argType="boolean"
            android:defaultValue="true" />
    </fragment>
    <fragment
        android:id="@+id/registrarCitaFragment"
        android:name="com.nutrizulia.presentation.view.RegistrarCitaFragment"
        android:label="Datos de la Cita"
        tools:layout="@layout/fragment_registrar_cita">
        <argument
            android:name="idPaciente"
            app:argType="string" />
        <argument
            android:name="idConsulta"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="isEditable"
            app:argType="boolean"
            android:defaultValue="true" />
    </fragment>
    <fragment
        android:id="@+id/registrarConsultaFragment"
        android:name="com.nutrizulia.presentation.view.RegistrarConsultaFragment"
        android:label="Registar Consulta"
        tools:layout="@layout/fragment_registrar_consulta">
        <argument
            android:name="idConsulta"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="isEditable"
            app:argType="boolean"
            android:defaultValue="true" />
        <argument
            android:name="idPaciente"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/seleccionarPacienteCitaFragment"
        android:name="com.nutrizulia.presentation.view.SeleccionarPacienteCitaFragment"
        android:label="Selecciona un Paciente"
        tools:layout="@layout/fragment_seleccionar_paciente_cita">
        <action
            android:id="@+id/action_seleccionarPacienteCitaFragment_to_registrarCitaFragment"
            app:destination="@id/registrarCitaFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/accionesPacienteFragment"
        android:name="com.nutrizulia.presentation.view.AccionesPacienteFragment"
        android:label="Gestionar Paciente"
        tools:layout="@layout/fragment_acciones_paciente">
        <argument
            android:name="idPaciente"
            app:argType="string" />
        <action
            android:id="@+id/action_accionesPacienteFragment_to_historiaPacienteFragment"
            app:destination="@id/historiaPacienteFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_accionesPacienteFragment_to_registrarPacienteFragment"
            app:destination="@id/registrarPacienteFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/historiaPacienteFragment"
        android:name="com.nutrizulia.presentation.view.HistoriaPacienteFragment"
        android:label="Historia Medica"
        tools:layout="@layout/fragment_historia_paciente">
        <argument
            android:name="idPaciente"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/accionesCitaFragment"
        android:name="com.nutrizulia.presentation.view.AccionesCitaFragment"
        android:label="Gestionar Consulta"
        tools:layout="@layout/fragment_acciones_cita" >
        <action
            android:id="@+id/action_accionesCitaFragment_to_registrarConsultaFragment"
            app:destination="@id/registrarConsultaFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <argument
            android:name="idConsulta"
            app:argType="string" />
        <action
            android:id="@+id/action_accionesCitaFragment_to_registrarCitaFragment"
            app:destination="@id/registrarCitaFragment" />
    </fragment>
    <fragment
        android:id="@+id/accionesConsultaFragment"
        android:name="com.nutrizulia.presentation.view.AccionesConsultaFragment"
        android:label="fragment_acciones_consulta"
        tools:layout="@layout/fragment_acciones_consulta" >
        <argument
            android:name="idConsulta"
            app:argType="string" />
        <action
            android:id="@+id/action_accionesConsultaFragment_to_verConsultaFragment"
            app:destination="@id/verConsultaFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_accionesConsultaFragment_to_editarConsultaFragment"
            app:destination="@id/editarConsultaFragment" />
    </fragment>
    <fragment
        android:id="@+id/verConsultaFragment"
        android:name="com.nutrizulia.presentation.view.VerConsultaFragment"
        android:label="Detalles de la Consulta"
        tools:layout="@layout/fragment_ver_consulta">
        <argument
            android:name="idConsulta"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/editarConsultaFragment"
        android:name="com.nutrizulia.presentation.view.EditarConsultaFragment"
        android:label="fragment_editar_consulta"
        tools:layout="@layout/fragment_editar_consulta" >
        <argument
            android:name="idConsulta"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/accionesCitaPerdidaFragment"
        android:name="com.nutrizulia.presentation.view.AccionesCitaPerdidaFragment"
        android:label="Gestionar Consulta"
        tools:layout="@layout/fragment_acciones_cita_perdida" >
        <argument
            android:name="idConsulta"
            app:argType="string" />
        <action
            android:id="@+id/action_accionesCitaPerdidaFragment_to_registrarCitaFragment"
            app:destination="@id/registrarCitaFragment" />
    </fragment>
    <fragment
        android:id="@+id/seleccionarInstitucionFragment"
        android:name="com.nutrizulia.presentation.view.SeleccionarInstitucionFragment"
        android:label="fragment_seleccionar_institucion"
        tools:layout="@layout/fragment_seleccionar_institucion" />
</navigation>